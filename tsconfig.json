{
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
  },
  "exclude": ["node_modules"]
}
